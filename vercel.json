{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.5"
        }
    },
    "routes": [
        { "src": "/", "dest": "/api/index.php" },
        {
            "src": "/index.php",
            "headers": { "Location": "/api/index.php" },
            "dest": "/api/index.php"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/public/*/(.*)",
            "dest": "/*/$1"
        }
    ]
}
